	<script>
	let slideIndex = 0;
	showSlides();

function showSlides() {
	  let i;
	  let slides = document.getElementsByClassName("mySlides");
	  let dots = document.getElementsByClassName("dot");
	  for (i = 0; i < slides.length; i++) {
	    slides[i].style.display = "none";  
	  } //Else image will be aligned vertically one after other
	  slideIndex++;
	  if (slideIndex > slides.length) {slideIndex = 1}    
	  for (i = 0; i < dots.length; i++) {
	    dots[i].className = dots[i].className.replace(" active", "");
	  }  // current dot will be active
	  slides[slideIndex-1].style.display = "block";  
	  dots[slideIndex-1].className += " active";
	  setTimeout(showSlides, 3000); // Change image every 3 seconds
}

function myFunction() {
    var input, filter, ul, li, a, i, txtValue;
    input = document.getElementById("myInput");
    filter = input.value.toUpperCase();		//Converting i/p to uppercase
    ul = document.getElementById("myUL");
    li = ul.getElementsByTagName("li");
    for (i = 0; i < li.length; i++) {
        a = li[i].getElementsByTagName("a")[0];		//Check the i/p index of list i
        txtValue = a.textContent;		//storing the content a in textValue
        if (txtValue.toUpperCase().indexOf(filter) > -1) {		// if textValue present in fiter on any index
            li[i].style.display = "";
            ul.style.display = "block";
        } else {
            li[i].style.display = "none";
        }
    }
}
//https://www.w3schools.com/howto/howto_js_dropdown.asp
//https://www.w3schools.com/howto/tryit.asp?filename=tryhow_js_filter_list
